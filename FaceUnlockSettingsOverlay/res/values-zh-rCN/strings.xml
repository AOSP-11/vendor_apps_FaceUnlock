<resources>

    <!-- Button shown which shows accessibility toggles for face enrollment when clicked. [CHAR LIMIT=32] -->
    <string name="security_settings_face_enroll_introduction_accessibility">手机正对脸部进行扫描</string>
    <!-- Additional details shown when the accessibility toggle is expanded. [CHAR LIMIT=NONE]-->
    <string name="security_settings_face_enroll_introduction_accessibility_expanded">如果您的视力或头部运动受限，则此选项是设置面部解锁的一种简便方法。使用面部解锁时，您的手机需要直接放到面前。</string>
    <!-- Message shown for a toggle which when enabled, allows the user to enroll using a simpler flow for accessibility [CHAR LIMIT=NONE] -->
    <string name="security_settings_face_enroll_introduction_accessibility_diversity">使用辅助功能脸部解锁</string>
    <!-- Message shown for a toggle which when enabled, allows the user to enroll using a simpler flow for accessibility [CHAR LIMIT=NONE] -->
    <string name="security_settings_face_enroll_introduction_accessibility_vision">需要睁开眼睛</string>
    <!-- Introduction title shown in face enrollment to introduce the face unlock feature [CHAR LIMIT=40] -->
    <string name="security_settings_face_enroll_introduction_title">设置面部解锁</string>
    <string name="security_settings_face_enroll_introduction_footer_part_0">人脸解锁注意事项</string>
    <!-- Introduction message shown in face enrollment that provides extra detail. [CHAR LIMIT=NONE] -->
    <string name="security_settings_face_enroll_introduction_footer_part_1">人脸解锁记录的数据将安全存储，您可以随时在设置中删除人脸解锁数据\n人脸解锁会使用最近的面部解锁图像来改进的面部模型</string>
    <!-- Introduction message shown in face enrollment that provides extra detail. [CHAR LIMIT=NONE] -->
    <string name="security_settings_face_enroll_introduction_footer_part_2">无意看向手机可能会将其解锁（别人拿起手机正对您的面部也会将其解锁）</string>
    <!-- Introduction message shown in face enrollment that provides extra detail. [CHAR LIMIT=NONE] -->
    <string name="security_settings_face_enroll_introduction_footer_part_3">长相很想您的人（如您的孩子或同胞胎兄弟姐妹）可以解锁您的手机</string>

    <!-- packages_apps_Settings -->
    <string name="security_settings_face_enroll_finish_title">人脸已添加</string>
</resources>
